{
"manifest_version": 2,
"name": "Give",
 "version": "1.0",
 "content_scripts": [
   {
     "matches": [
       "https://www.amazon.com/*",
       "https://donate.unicefusa.org/*"
     ],

     "js": ["jquery-3.3.1.min.js","content.js"]

   }
 ],
 "permissions": [
"storage",
"tabs"
],

 "browser_action": {
   "default_icon": "donate.png",
   "default_popup": "popup.html"
 }

}
